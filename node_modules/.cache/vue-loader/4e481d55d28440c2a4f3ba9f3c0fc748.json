     }\n    },\n    watch: {\n      $route: {\n        immediate: true,\n        handler: function () {\n          this.updateBackground()\n        }\n      }\n    }\n  };\n</script>\n<style lang=\"scss\">\n  $scaleSize: 0.8;\n  @keyframes zoomIn8 {\n    from {\n      opacity: 0;\n      transform: scale3d($scaleSize, $scaleSize, $scaleSize);\n    }\n    100% {\n      opacity: 1;\n    }\n  }\n\n  .main-content .zoomIn {\n    animation-name: zoomIn8;\n  }\n\n  @keyframes zoomOut8 {\n    from {\n      opacity: 1;\n    }\n    to {\n      opacity: 0;\n      transform: scale3d($scaleSize, $scaleSize, $scaleSize);\n    }\n  }\n\n  .main-content .zoomOut {\n    animation-name: zoomOut8;\n  }\n</style>\n"]}]}